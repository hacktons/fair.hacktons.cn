{
  "className": "Scaffold",
  "na": {
    "appBar": {
      "className": "AppBar",
      "na": {
        "title": {
          "className": "Text",
          "pa": [
            "Flutter Fair Example"
          ]
        },
        "actions": [
          {
            "className": "IconButton",
            "na": {
              "icon": {
                "className": "Icon",
                "pa": [
                  "#(Icons.show_chart)"
                ]
              },
              "onPressed": "#(onTapSetting)"
            }
          }
        ]
      }
    },
    "body": {
      "className": "Center",
      "na": {
        "child": {
          "className": "Column",
          "na": {
            "mainAxisAlignment": "#(MainAxisAlignment.center)",
            "children": [
              {
                "className": "Image.asset",
                "pa": [
                  "assets/image/logo.png"
                ]
              },
              {
                "className": "Padding",
                "na": {
                  "padding": {
                    "className": "EdgeInsets.only",
                    "na": {
                      "bottom": 40
                    }
                  },
                  "child": {
                    "className": "Text",
                    "pa": [
                      "#(Running with $_platform, click count = $_count)"
                    ]
                  }
                }
              }
            ]
          }
        }
      }
    },
    "floatingActionButton": {
      "className": "Row",
      "na": {
        "mainAxisAlignment": "#(MainAxisAlignment.end)",
        "children": [
          {
            "className": "FloatingActionButton",
            "na": {
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.open_in_new)"
                ]
              },
              "heroTag": "open_dynamic",
              "backgroundColor": "#(Colors.pink)",
              "onPressed": {
                "className": "Navigator.pushNamed",
                "pa": [
                  "context",
                  "sample_dynamic_page"
                ]
              }
            }
          },
          {
            "className": "Padding",
            "na": {
              "padding": {
                "className": "EdgeInsets.only",
                "na": {
                  "left": 20,
                  "right": {
                    "className": "Sugar.paddingTop",
                    "pa": [
                      "context"
                    ]
                  }
                }
              }
            }
          },
          {
            "className": "FloatingActionButton",
            "na": {
              "child": {
                "className": "Icon",
                "pa": [
                  "#(Icons.add)"
                ]
              },
              "onPressed": "#(onTapText)"
            }
          }
        ]
      }
    }
  }
}